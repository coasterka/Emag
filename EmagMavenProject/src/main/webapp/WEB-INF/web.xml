<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SignIn</servlet-name>
  	<display-name>SignIn</display-name>
  	<description></description>
  	<servlet-class>servlets.SignIn</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CheckUserResult</servlet-name>
  	<display-name>CheckUserResult</display-name>
  	<description></description>
  	<servlet-class>servlets.CheckUserResult</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SignUp</servlet-name>
  	<display-name>SignUp</display-name>
  	<description></description>
  	<servlet-class>servlets.SignUp</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreateCategory</servlet-name>
  	<display-name>CreateCategory</display-name>
  	<description></description>
  	<servlet-class>servlets.CreateCategory</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SignIn</servlet-name>
  	<url-pattern>/SignIn</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CheckUserResult</servlet-name>
  	<url-pattern>/CheckUserResult</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignUp</servlet-name>
  	<url-pattern>/SignUp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateCategory</servlet-name>
  	<url-pattern>/CreateCategory</url-pattern>
  </servlet-mapping>
</web-app>
